#Notes
import math ;
import numpy as np;
import pandas as pd;
import matplotlib.pyplot as plot;
import csv
import csv
with open('wqdata', newline='') as f:
  reader = csv.reader(f)
  data = list(reader)
data = np.array(data)
dataset = []
for i in data:
  for a in i:
    a = a.split(";")
    dataset.append(a)
dataset = np.array(dataset)

size = [len(dataset),len(dataset[0])] #Height.
print(size)

df = pd.DataFrame((dataset[1::,:]),columns=dataset[0])
print ("dataset = ")
print (df)

features = (dataset[:,0:-2])#Features
targets = dataset[:,-1::]#Targets

print ("Normal Equation Linear Regression Method")
X = features
Y = targets
print ("X =", X);
print ("Y=", Y);
theta = (np.linalg.inv((np.transpose(X))*X))*np.transpose(X)*Y;
print ("theta=", theta)



